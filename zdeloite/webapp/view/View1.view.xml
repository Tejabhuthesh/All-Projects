        <headerContent>
            <Button
                text="Special Closing"
                type="Ghost"
                icon="sap-icon://forward"
            />
            <Button
                text="Payroll Closing"
                type="Ghost"
                icon="sap-icon://collections-insight"
            />
            <Button
                text="Actions"
                type="Ghost"
                icon="sap-icon://action"
            />
        </headerContent>

        <content>
    <Panel >
      <Title text="Capture Filters" />
    <HBox class="flex-parent" >
    <VBox  class="box">
  <HBox>
                    <Label
                        text="Stores"
                        class="CF"
                    />
                    <ComboBox
                        width="12rem"
                        items="{
			            	path: 'Data2>/results',
				            sorter: { path: 'Name1' } }"
                    >
                        <core:Item text="{Data2>Name1}" />
                    </ComboBox>

                    <Label
                        text="Dates"
                        class="CF"
                    />
                    <ComboBox
                        width="12rem"
                        items="{
			            	path: 'Data2>/results',
				            sorter: { path: 'Name1' } }"
                    >
                        <core:Item text="{Data2>Name1}" />
                    </ComboBox>
                    <Button
                        text="Filter"
                        icon="sap-icon://search"
                        class="CF1"
                    />
                   
                </HBox>
    </VBox>
    <VBox class="">
      <!-- <OverflowToolbar
                    design="Transparent"
