                        <Label
                            class="CF4"
                            text="Rest"
                            wrapping="true"
                        />
                    </HBox>
                    <HBox>
                        <core:Icon
                            src="sap-icon://color-fill"
                            class="validationOK"
                        />
                        <Label
                            class="CF4"
                            text="validation OK"
                            wrapping="true"
                        />
                    </HBox>
                    <HBox>
                        <core:Icon
                            src="sap-icon://color-fill"
                            class="ProposedIncidence"
                        />
                        <Label
                            class="CF4"
                            text="Proposed Incidence"
                            wrapping="true"
                        />
                    </HBox>
                    <HBox>
                        <core:Icon
                            src="sap-icon://color-fill"
                            class="PendingAuthorizationByAC"
                        />
                        <Label
                            class="CF4"
                            text="Pending authorization by AC"
                            wrapping="true"
                        />
                    </HBox>
                    <HBox>
                        <core:Icon
                            src="sap-icon://color-fill"
