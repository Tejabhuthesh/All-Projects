     <HBox>
                        <Label
                            class="font_L "
                            text="Legend"
                            wrapping="true"
                        />
                       
                    </HBox>
                    <HBox>
                        <core:Icon
                        src="sap-icon://alert"
                        size="1.5rem"
                        class="Vacation"
                    />
